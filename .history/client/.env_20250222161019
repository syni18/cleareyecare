# server url
VITE_SERVER_URL=https://cleareyecare-production.up.railway.app

# product dummy json url
VITE_DUMMY_JSON_URL=https://dummyjson.com/products/?limit=50

# validation Regular expression
VITE_REGEX_NAME=/^[a-zA-Z]+$/
VITE_REGEX_EMAIL=/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
VITE_REGEX_UPPERCASE=/[A-Z]/
VITE_REGEX_LOWERCASE=/[a-z]/
VITE_REGEX_DIGITS=/\d/
VITE_REGEX_SPECIALCHAR=/[-._!"`'#%&,:;<>=@{}~\$\(\)\*\+\/\\\?\[\]\^\|]+/
VITE_REGEX_PANNO=/^[A-Z]{5}[0-9]{4}[A-Z]$/
